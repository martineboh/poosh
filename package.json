{
  "private": true,
  "name": "poosh",
  "description": "Publish local files to virtually any remote endpoint (AWS S3)",
  "author": "Merlicco Yves",
  "repository": "https://github.com/yvele/poosh",
  "keywords": [
    "poosh",
    "s3",
    "aws",
    "publish",
    "publisher",
    "sync"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yvele/poosh/issues"
  },
  "homepage": "https://github.com/yvele/poosh",
  "scripts": {
    "bootstrap": "npm install && lerna bootstrap",
    "bootstrap:yarn": "yarn && lerna bootstrap",
    "build": "./scripts/build.sh",
    "clean": "npm run clean:build && npm run clean:test && npm run clean:npm",
    "clean:build": "./scripts/clean-build.sh",
    "clean:npm": "./scripts/clean-npm.sh",
    "clean:test": "./scripts/clean-test.sh",
    "coverage": "./scripts/coverage.sh",
    "outdated": "./scripts/outdated.sh",
    "publish": "./scripts/publish.sh",
    "style": "./scripts/style.sh",
    "test": "./scripts/test.sh",
    "validate": "npm run style && npm run test"
  },
  "engines": {
    "node": ">=4.3.2",
    "npm": ">=v4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-flow-comments": "^6.3.19",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-register": "^6.22.0",
    "eslint": "^3.14.0",
    "eslint-config-poosh": "^1.0.8",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "lerna": "2.0.0-beta.32",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rewire": "^2.5.2",
    "should": "^11.1.2"
  },
  "nyc": {
    "reporter": [
      "text",
      "text-summary",
      "lcov",
      "html"
    ],
    "include": [
      "packages/*/src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true
  }
}
